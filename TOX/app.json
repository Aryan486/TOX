{
  "expo": {
    "name": "TOX",
    "slug": "TOX",
    "version": "1.2.0",
    "icon": "./assets/mainLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mainLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#EFD6BE"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notificationLogo.png",
          "color": "#EFD6BE",
          "sounds": [
            "./assets/sharp.wav"
          ]
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist":{
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.aryanb.TOX"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mainLogo.png",
        "backgroundColor": "#EFD6BE"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.aryanb.TOX"
    }
  }
}
